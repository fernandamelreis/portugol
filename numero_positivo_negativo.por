programa {
  funcao inicio() {
    inteiro num 

    escreva("Digite um número: ")
    leia(num)

    se (num > 0){
      escreva("O número digitado é positivo! \n")
    }
    se (num == 0){
      escreva("O número digitado é neutro! \n")
    }
    se (num < 0 ){
      escreva("O número digitado é negativo! \n")
    }
  }
}
